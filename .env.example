## Copy to `.env` and fill with your Supabase project keys.

# Supabase
VITE_SUPABASE_URL=https://YOUR-PROJECT-REF.supabase.co
VITE_SUPABASE_ANON_KEY=YOUR_ANON_KEY

# Web Push (optional)
VITE_VAPID_PUBLIC_KEY=YOUR_VAPID_PUBLIC_KEY

# Dev-only: bypasses client UI auth gates (does NOT bypass RLS!)
VITE_DEV_NO_AUTH=true
VITE_MOCK_MEMORIALS=true
